            "NoEcho": "true",
            "Type": "String"
        },
        "WISUser": {
            "Default": "",
            "Description": "The user name (in Active Directory in your Windows network) for the user who has privileges to access PI Data Archive and AF Server. Required only if Authentication Method is set to \"WIS\".",
            "Type": "String"
        },
        "WorkerInstanceType": {
            "AllowedValues": [
                "t2.micro",
                "t2.small",
                "t2.medium",
                "t2.large",
                "m4.large",
