                    ]
                },
                {
                    "Label": {
                        "default": "Splunk Configuration"
                    },
                    "Parameters": [
                        "SplunkInstanceType",
                        "SearchHeadInstanceType",
                        "SplunkSyslogInstanceType",
                        "IndexerInstanceType",
                        "SHCEnabled",
                        "SplunkIndexerCount",
                        "SplunkIndexerDiskSize",
                        "SplunkReplicationFactor",
                        "SplunkSyslogInstanceCount",
                        "SplunkSyslogDiskSize",
                        "IndexerApps",
                        "SearchHeadApps",
                        "SplunkUFLocation",
                        "SplunkLicenseBucket",
                        "SplunkLicensePath",
                        "SplunkAdminPassword",
                        "SplunkSecret",
